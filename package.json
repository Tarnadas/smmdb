    "@types/compression": "^0.0.36",
    "@types/cookie-session": "^2.0.34",
    "@types/express": "^4.11.1",
    "@types/file-type": "^5.2.1",
    "@types/history": "^4.6.2",
    "@types/ini": "^1.3.29",
    "@types/jest": "^22.2.2",
    "@types/marked": "^0.3.0",
